event(sub GetToken{ local( $LEVEL, $PREV_LEVEL, $CODE , $PREV_CODE ) = @_;);//
set($BRANCH = "(if|try)";);//
set($INPUT = "(def|class)";);//
set();//
set($LOOP = "^\t*(for|while|do)";);//
set($WITH = "^\t*(with)";);//
set($PEND = "^\t*(else|except)";);//
set($END = "^\t*(break|continue|return)";);//
set($OUTPUT = "^\t*(print)";);//
set($EVENT = "^(import|from)";);//
set($PATH = "^\t*(else|elif)";);//
generic(local( $DIFF )  = $LEVEL - $PREV_LEVEL ;);// 
set(local( $TYPE ) = "...";);//
set(local( $TOKEN ) = "...";);//
branch(if ($DIFF > 0  ));//
path( { );//
branch(if ( $PREV_CODE =~m/$INPUT/ ));//
path( { );//
set($TYPE = "end( );\/\/$PREV_LINE " ;);//
process($TOKEN = "input( $PREV_CODE);\/\/  LEVEL($PREV_LEVEL)  "  ;);//
path(} else { );//
branch(if ( $PREV_CODE =~ m/$BRANCH/ ));//
path( { );//
set($TYPE = "bend(  );\/\/  $PREV_LINE " ;);//
process($TOKEN = "branch( $PREV_CODE);\/\/  LEVEL($PREV_LEVEL)  "  ;);//
path(} else { );//
set($TYPE = "set( );\/\/$PREV_LINE " ;);//
process($TOKEN = "set( $PREV_CODE);\/\/  LEVEL($PREV_LEVEL)  "  ;);//
bend( } );//
bend( } );//
event(push( @stack, "$TYPE <<<$PREV_CODE"  ););////push( @stack,"$PREV_LINE LEVEL($PREV_LEVEL)  "  );//
output();//print "\tPUSH $PREV_CODE\n ";
path(} else { );//
branch(if ($DIFF <  0  ));//
path( { );//
event($POP = pop( @stack ) ;);//////
output();//print "\tPOP  $POP\n ";
process($TOKEN = "$POP  <---- popped"  ;);//$TOKEN = "pop <------------------------------------- $POP  from   $PREV_LINE LEVEL($PREV_LEVEL) "  ;
path(} else { );//
set($TOKEN = ""  ;);//$TOKEN = "$CODE"  ;
branch(if ( $CODE =~ m/$PATH/ ));//
path( { );//
process($TOKEN = "path( $CODE);\/\/  LEVEL($LEVEL)  "  ;);//
path(} else { );//
process($TOKEN = "set( $CODE);\/\/  LEVEL($LEVEL)  "  ;);//
bend( } );//
bend( } );//
bend( } );//
branch(if ( $DIFF * $DIFF  > 1 ));//
path( { );//
process($TOKEN = "$TOKEN <---! ERROR IN DIFF = $DIFF "  ;);//
path(} else { );//
bend( } );//
end( return $TOKEN;  });//
