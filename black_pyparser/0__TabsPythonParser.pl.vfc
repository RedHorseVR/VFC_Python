input(sub Parse{);//
output(open OUTFILE,  ">" ,   $outputVFC  or die "Cannot open output: $!";);//
set(  open( FILE, $cmd_line ););//
output(print( "...parseing $cmd_line\n" ););//
generic($lastCount = 0;);// ////
set(  $ParsedFile  = "";);//
set($multiLine = 0;);//
set($LINE = 0;);//
generic($PREV_LEVEL = 0;);// ////
generic($PREV_LINE = 0;);//  
generic($LEVEL = 0;);//  
generic(local($DIFF) = 0;);//  
loop(  while(<FILE>) {);//
set($LINE = $LINE + 1 ;);//
branch(if (     ( $_ =~m/\"\"\"/   || $_ =~m/\'\'\'/)   ));//
path( { );//
set($multiLine = ~$multiLine;);//
bend( } );//
branch(if ( $_ =~ m/^\n$/  ||  $multiLine ));//skip all processing of all blank lines
path( { );//
branch(if ( $multiLine ));//
path( { );//
generic($LEVEL = 0;);//  
output(print( "set( );\/\/ $_" ););//print( "$LINE ### \n" );
path(} else { );//
output(print( "set();\/\/ $LINE\n" ););//
bend( } );//
path( }else{ );//
output();//print( $CODE,"\n" );
set();//
process($LEVEL = getIndents( $_  ) ;);//////
set($COMM = getComment( $_ ) ;);//
set($CODE = getCode( $_ ) ;);//
generic($DIFF = $PREV_LEVEL -  $LEVEL ;);//  
branch(if ( $DIFF   > 1   ));//if ( $DIFF   > 1   && $LEVEL != 0 )
path( {);//popping ...
loop(while( $DIFF > -1  ) {);//
set($DIFF =  $DIFF -1 ;);//
set($BACK_LEVEL  =  $LEVEL + $DIFF;);//
set($BACK_DIFF  =  $BACK_LEVEL - $PREV_LEVEL ;);//
process($TOKEN = GetToken( $BACK_LEVEL, $PREV_LEVEL , $CODE , $PREV_CODE ););//
process(print_extra_TOKENS( $TOKEN ););//
output( print( "$TOKEN\n" ););//print( $LINE,"<$BACK_DIFF>" ); PrintTabs( $BACK_LEVEL   ); print( $BACK_LEVEL  ,"** $TOKEN\n" );
set();//$PREV_LEVEL = $BACK_LEVEL;
lend(});//
path( }else{);//pushing ...
output(print( "set($CODE);\/\/<<<<<<<<\n" ););//
process($TOKEN = GetToken( $LEVEL, $PREV_LEVEL , $CODE , $PREV_CODE ););//
output(print("$TOKEN \n" ) ;);//print( $LINE ); print("<$DIFF>"); PrintTabs( $LEVEL ); print( " $LEVEL " );  print("$TOKEN \n" ) ;
process(print_extra_TOKENS( $TOKEN ););//
bend( } );//
bend( } );//
set($PREV_LEVEL = $LEVEL;);//
generic($PREV_LINE = $LINE;);//  
generic($PREV_CODE = $CODE;);//  
lend(  });//# End of SEARCHFILE
generic(  close ( FILE ););//
generic(  close ( OUTFILE ););//
end( });// 

